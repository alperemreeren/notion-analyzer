{
  "version": 2,
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health.ts"
    },
    {
      "src": "/analyze",
      "dest": "/api/analyze.ts"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Authorization, Content-Type" }
      ]
    }
  ]
}
